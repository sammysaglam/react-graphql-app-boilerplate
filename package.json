{
  "name": "empty-project-template",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "start": "yarn gql-flow && USE_WEBPACKDEV_SERVER=true nodemon src/server/server.js --watch src/server --ext js,graphql & webpack-dev-server --hot --history-api-fallback --env.hot=true --open & expo start",
    "build": "yarn gql-flow && webpack --env.production=true",
    "analyze": "yarn gql-flow && webpack --env.analyze=true",
    "lint": "\"./node_modules/.bin/eslint\" .",
    "lint:fix": "\"./node_modules/.bin/eslint\" . --fix",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "serve:start": "pm2 start build/server/server.js",
    "serve:stop": "pm2 stop server && pm2 delete server",
    "serve": "node build/server/server.js",
    "watch": "webpack --progress --watch & nodemon build/server/server.js --watch build/server",
    "gql-flow": "yarn apollo schema:download gql-schema.temp.json --endpoint=src/server/schema.graphql && yarn apollo codegen:generate --queries=src/client/graphql/**/*.graphql --schema=gql-schema.temp.json --target=flow --addTypename --useFlowExactObjects"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.3.10",
    "apollo-client": "^2.4.6",
    "apollo-link": "^1.2.3",
    "apollo-link-http": "^1.5.5",
    "apollo-server-express": "^2.1.0",
    "axe-prop-types": "^1.0.8",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cookies": "^0.7.2",
    "cors": "^2.8.4",
    "dotenv": "^6.1.0",
    "empty": "^0.10.1",
    "exenv": "^1.2.2",
    "expo": "^31.0.2",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "express-ws": "^4.0.0",
    "graphql": "^0.13.2",
    "graphql-tools": "^4.0.3",
    "helmet": "^3.14.0",
    "node-fetch": "^2.2.0",
    "nodemailer": "^4.6.8",
    "object-filter": "^1.0.2",
    "object.map": "^1.0.1",
    "pm2": "^3.2.2",
    "prop-types": "^15.6.2",
    "react": "16.5.0",
    "react-apollo": "^2.2.4",
    "react-dom": "16.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-31.0.0.tar.gz",
    "react-router-dom": "^4.3.1",
    "reduce-object": "^0.1.3",
    "styled-components": "^4.0.3",
    "ws": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "apollo": "^1.9.2",
    "axe-markdown-loader": "^2.0.5",
    "babel-loader": "^8.0.4",
    "babel-preset-expo": "^5.0.0",
    "chromedriver": "^2.43.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "dotenv-webpack": "^1.5.7",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.8.0",
    "eslint-sammy-preset": "^7.3.0",
    "flow-bin": "^0.85.0",
    "flow-typed": "^2.5.1",
    "geckodriver": "^1.12.2",
    "generate-asset-webpack-plugin": "^0.3.0",
    "html-minifier": "^3.5.21",
    "imagemin-webpack-plugin": "^2.3.0",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jest-transform-graphql": "^2.1.0",
    "jsonminify": "^0.4.1",
    "nodemon": "^1.18.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "react-hot-loader": "^4.3.12",
    "sass-loader": "^7.1.0",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "style-loader": "^0.23.1",
    "svg-react-loader": "^0.4.6",
    "url-loader": "^1.1.2",
    "webpack": "^4.24.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      ".*": "babel-jest"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/tests-config/setup.js"
  }
}
