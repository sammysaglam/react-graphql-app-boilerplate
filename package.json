{
	"private": true,
	"main": "node_modules/expo/AppEntry.js",
	"scripts": {
		"start": "npm run watch:schema & USE_WEBPACKDEV_SERVER=true ts-node-dev ./src/web-app/server/server.ts --transpileOnly --watch-extensions ts,tsx,graphql & webpack-dev-server --env.hot=true",
		"start:mobile": "expo start",
		"prewatch": "mkdirp ./build/server/ && echo // init > ./build/server/server.js",
		"watch": "npm run watch:schema & webpack --watch & nodemon ./build/server/server",
		"watch:schema": "nodemon scripts/build-schema.js --watch src/web-app --ext graphql --ignore server/__generated__/schema.graphql",
		"build:schema": "node scripts/build-schema.js",
		"build": "npm run build:schema && webpack --env.production=true",
		"serve": "node ./build/server/server",
		"lint": "\"./node_modules/.bin/eslint\" . --ext js,ts,tsx"
	},
