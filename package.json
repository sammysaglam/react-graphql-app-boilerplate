{
		"private": true,
		"main": "node_modules/expo/AppEntry.js",
		"scripts": {
				"start": "npm run watch:schema & USE_WEBPACKDEV_SERVER=true ts-node-dev ./src/web-app/server/server.ts --transpileOnly --watch-extensions ts,tsx,graphql & webpack-dev-server --env.hot=true --config webpack.config.web-app.js",
				"start:mobile": "expo start",
				"prewatch": "solidarity && npm run build:schema && mkdirp ./build/server/ && echo // init > ./build/server/server.js",
				"watch": "npm run watch:schema & webpack --watch --config webpack.config.web-app.js & nodemon ./build/server/server",
				"watch:schema": "nodemon scripts/build-schema.js --watch src/web-app --ext graphql --ignore server/__generated__/schema.graphql",
				"build:schema": "node scripts/build-schema.js",
				"build": "npm run build:schema && webpack --config webpack.config.web-app.js --env.production=true",
				"analyze": "webpack --config webpack.config.web-app.js --env.analyze=true",
				"serve": "node ./build/server/server",
				"lint": "\"./node_modules/.bin/eslint\" . --ext js,ts,tsx",
				"prestart": "solidarity && npm run build:schema",
				"prestart:mobile": "solidarity && npm run build:schema",
				"prebuild": "solidarity",
				"preanalyze": "solidarity",
				"preinstall": "npx solidarity"
		},
