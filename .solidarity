{
	"$schema": "http://json.schemastore.org/solidaritySchema",
	"config": {
		"output": "verbose"
	},
	"requirements": {
		"Node": [
			{
				"rule": "cli",
				"binary": "node",
				"semver": "12.10.0",
				"error": "You have node@{{installedVersion}}, and need node@{{wantedVersion}}. Fix with `nvm install {{wantedVersion}} && nvm use`"
			}
		],
		"NPM": [
			{
				"rule": "cli",
				"binary": "npm",
				"semver": "6.10.3",
				"error": "You have npm@{{installedVersion}}, and need npm@{{wantedVersion}}."
			}
		],
		"Yarn": [
			{
				"rule": "cli",
				"binary": "yarn",
				"semver": "1.17.3",
				"error": "You have yarn@{{installedVersion}}, and need yarn@{{wantedVersion}}. Fix with `npm install -g yarn@{{wantedVersion}}`"
			}
		]
	}
}
